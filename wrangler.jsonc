{
	"$schema": "node_modules/wrangler/config-schema.json",
	"name": "planning-poker",
	"compatibility_date": "2025-06-28",
	"compatibility_flags": ["nodejs_compat"],
	"main": "./worker.ts",
	"assets": {
		"binding": "ASSETS",
		"directory": "./.output/public/"
	},
	"durable_objects": {
		"bindings": [
			{
				"name": "POKER_ROOM",
				"class_name": "PokerRoom"
			}
		]
	},
	"migrations": [
		{
			"tag": "v1",
			"new_classes": ["PokerRoom"]
		}
	],
	"observability": {
		"enabled": true,
		"logs": {
			"enabled": true,
			"head_sampling_rate": 1,
			"persist": true,
			"invocation_logs": true
		},
		"traces": {
			"enabled": true,
			"head_sampling_rate": 1,
			"persist": true
		}
	}
}
